{
    "name": "Sessions",
    "version": "0.1.0",
    "description": "A web portal where you can book one-on-one video calls with a variety of experts in different fields.",
    "repository": {
        "type": "git",
        "url": "https://github.com/yonashalefom/sessions-backend"
    },
    "author": {
        "name": "Yonas Halefom",
        "email": "contact@yonashalefom.com"
    },
    "type": "commonjs",
    "private": true,
    "license": "MIT",
    "scripts": {
        "prepare": "husky",
        "clean": "rimraf dist && rimraf node_modules && yarn cache clean",
        "build": "nest build",
        "format": "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}' && prettier --write '*.{ts,json,yml,yaml,mjs}'",
        "start": "cls && nest start",
        "start:dev": "cls && nest start --watch src/main.ts",
        "start:prod": "cls && node dist/main",
        "lint": "eslint",
        "lint:staged": "lint-staged",
        "lint:fix": "eslint --fix",
        "test": "set TZ=\"UTC\" && jest --config test/jest.json --passWithNoTests --forceExit",
        "deadcode": "ts-prune --project tsconfig.json --unusedInModule --skip \".*\\.json\"\n",
        "spell": "cspell lint --config cspell.json \"{src,test}/**/*.ts\" --color --gitignore --no-must-find-files --no-summary --no-progress",
        "package:upgrade": "npx npm-check-updates -u",
        "package:check": "npx npm-check-updates -i",
        "migrate:fresh": "yarn migrate:remove && yarn migrate:schema && yarn migrate:seed",
        "migrate:schema": "APP_ENV=migration nest start",
        "migrate:seed": "set APP_ENV=\"migration\" && nestjs-command seed:country && nestjs-command seed:apikey && nestjs-command seed:role && nestjs-command seed:user",
        "migrate:remove": "APP_ENV=migration nestjs-command remove:user && nestjs-command remove:country && nestjs-command remove:apikey  && nestjs-command remove:role",
        "migrate:template": "nestjs-command migrate:template",
        "rollback:template": "nestjs-command rollback:template"
    },
    "dependencies": {
        "@aws-sdk/client-pinpoint": "^3.758.0",
        "@aws-sdk/client-s3": "^3.758.0",
        "@aws-sdk/client-ses": "^3.758.0",
        "@aws-sdk/s3-request-presigner": "^3.758.0",
        "@casl/ability": "^6.7.3",
        "@faker-js/faker": "^9.6.0",
        "@keyv/redis": "^4.3.2",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bullmq": "^11.0.2",
        "@nestjs/cache-manager": "^3.0.0",
        "@nestjs/common": "^11.0.12",
        "@nestjs/config": "^4.0.1",
        "@nestjs/core": "^11.0.12",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mongoose": "^11.0.2",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.12",
        "@nestjs/swagger": "^11.0.7",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@sentry/nestjs": "^9.7.0",
        "@sentry/profiling-node": "^9.7.0",
        "@stream-io/node-sdk": "^0.4.19",
        "axios": "^1.8.4",
        "bad-words": "^4.0.0",
        "bcryptjs": "^3.0.2",
        "bullmq": "^5.44.0",
        "cache-manager": "^6.4.1",
        "case": "^1.6.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.8.0",
        "crypto-js": "^4.2.0",
        "google-auth-library": "^9.15.1",
        "helmet": "^8.1.0",
        "joi": "^17.13.3",
        "libphonenumber-js": "^1.12.8",
        "luxon": "^3.5.0",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.47",
        "mongoose": "^8.12.1",
        "nestjs-command": "^3.1.4",
        "nestjs-i18n": "^10.5.1",
        "nestjs-pino": "^4.4.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pino-http": "^10.4.0",
        "pino-pretty": "^13.0.0",
        "reflect-metadata": "^0.2.2",
        "response-time": "^2.3.3",
        "rotating-file-stream": "^3.2.6",
        "rxjs": "^7.8.2",
        "slugify": "^1.6.6",
        "verify-apple-id-token": "^3.1.2",
        "xlsx": "^0.18.5",
        "yargs": "^17.7.2",
        "yarn": "^1.22.22"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.22.0",
        "@golevelup/ts-jest": "^0.6.2",
        "@nestjs/cli": "^11.0.5",
        "@nestjs/schematics": "^11.0.2",
        "@nestjs/testing": "^11.0.12",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.11.11",
        "@swc/jest": "^0.2.37",
        "@types/bad-words": "^3.0.3",
        "@types/bcryptjs": "^2.4.6",
        "@types/bytes": "^3.1.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/crypto-js": "^4.2.2",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^5.0.1",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.16",
        "@types/luxon": "^3.4.2",
        "@types/ms": "^2.1.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.10",
        "@types/passport-jwt": "^4.0.1",
        "@types/response-time": "^2.3.8",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "cspell": "^8.17.5",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "rxjs-marbles": "^7.0.1",
        "stop-only": "^3.4.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "ts-prune": "^0.10.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --write '{src,test}/**/*.{ts,json,yml,yaml}'",
            "eslint",
            "stop-only --file"
        ],
        "*.{json,yaml}": [
            "prettier --write"
        ]
    },
    "packageManager": "yarn@1.22.22",
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.22.22",
        "node": ">= 20.11.0"
    },
    "jest": {
        "transform": {
            "^.+\\.(t|j)s?$": [
                "@swc/jest"
            ]
        }
    }
}
